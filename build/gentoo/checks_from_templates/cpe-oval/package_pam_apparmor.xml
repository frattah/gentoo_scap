<def-group>
  <definition class="inventory" id="package_pam_apparmor"
  version="1"><metadata>
        <title>Package pam_apparmor is installed</title>
            <affected family="unix">
                <platform>multi_platform_all</platform>
            </affected>
        <description>The RPM package pam_apparmor should be installed.</description>
    </metadata>
    <criteria>
    
      <criterion comment="Platform package pam_apparmor is installed"
      test_ref="inventory_test_package_pam_apparmor_installed" />
    
    </criteria>
  </definition>


  <linux:rpminfo_test check="all" check_existence="all_exist"
  id="inventory_test_package_pam_apparmor_installed" version="1"
  comment="package pam_apparmor is installed">
    <linux:object object_ref="obj_inventory_test_package_pam_apparmor_installed" />
    
  </linux:rpminfo_test>
  <linux:rpminfo_object id="obj_inventory_test_package_pam_apparmor_installed" version="1">
    <linux:name>pam_apparmor</linux:name>
  </linux:rpminfo_object>
  


</def-group>