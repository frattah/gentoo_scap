<def-group>
  <definition class="compliance" id="package_s-nail_installed"
  version="1">
    <metadata>
        <title>The s-nail Package Is Installed</title>
            <affected family="unix">
                <platform>multi_platform_all</platform>
            </affected>
        <description>The RPM package s-nail should be installed.</description>
    </metadata>
    <criteria>
      <criterion comment="package s-nail is installed"
      test_ref="test_package_s-nail_installed" />
    </criteria>
  </definition>

  <linux:rpminfo_test check="all" check_existence="all_exist"
  id="test_package_s-nail_installed" version="1"
  comment="package s-nail is installed">
    <linux:object object_ref="obj_test_package_s-nail_installed" />
    
  </linux:rpminfo_test>
  <linux:rpminfo_object id="obj_test_package_s-nail_installed" version="1">
    <linux:name>s-nail</linux:name>
  </linux:rpminfo_object>
  

</def-group>